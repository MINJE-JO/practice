generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

enum Brand {
  WILSON
  BABOLAT
  HEAD
  YONEX
}

enum StringPattern {
  PATTERN_16x19
  PATTERN_16x20
  PATTERN_18x20
  PATTERN_14x16
}

model RacketDB {
  id            Int           @id @default(autoincrement())
  name          String
  brand         Brand         
  weight        Int           
  stringPattern StringPattern 
  stiffness    Int
  balance       Int
  headsize      Int
  swingweight   Int
}

model StringDB {
  id            Int           @id @default(autoincrement())
  name          String
  brand         Brand
  gauge         Int
  shape         String
  firmness      String
  durability    String
  color         String
}